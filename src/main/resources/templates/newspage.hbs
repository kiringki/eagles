                {{#if category3}} / {{category3}} {{/if}}
                {{#if category4}} / {{category4}} {{/if}}
                {{#if category5}} / {{category5}} {{/if}}
                {{#if category6}} / {{category6}} {{/if}}
                {{#if category7}} / {{category7}} {{/if}}
                {{#if category8}} / {{category8}} {{/if}}
