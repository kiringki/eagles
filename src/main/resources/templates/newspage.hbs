<style>
    .columns{
        columns: 3;
    }

    #floating-bar {
        height: 220px;
        width: 80px;
        position: fixed;
        right: 3%;
        top: 35%;
        margin: 0 0 0 110px;
        padding: 5px;
    }
    #floating-bar > button {
        height: 52px;
        width: 70px;
        margin-bottom: 5px; padding: 3px; border-radius: 7px;
    }

    #essaytitle{
        position: relative;
        background: #fff;
        text-align: center;
    }
    #essaytitle > .container {
        padding: 7em 0 3em 0;
        border-bottom: solid 2px #e5e5e5;
        box-shadow: inset 0px -8px 0px 0px #fff, inset 0px -10px 0px 0px #e5e5e5;
        text-align: center;
    }

    #essaytitle h3 {
        font-weight: 700;
        color: #ed786a;
        text-shadow: 0.05em 0.075em 0 rgba(0, 0, 0, 0.1);
        letter-spacing: 13px;
    }

    @media ( max-width: 1000px ) {
        #floating-bar{
            height: 80px;
            width: 45%;
            position:fixed;
            top:85%;
            bottom: 0;
            right: 25%;
            text-align: center;
            margin: 0 auto 0 auto;
        }
        #floating-bar > button{
            margin: 0 5px 0 5px;
            height: 52px;
            width: 20%;
            padding: 3px; border-radius: 7px;
            float: left;
        }
    }

    @media ( max-width: 800px ) {
        #floating-bar{
            width: 50%;
        }
        #floating-bar > button{
            font-size: 10pt;
        }
    }
    @media ( max-width: 670px ) {
        #floating-bar{
            right: 10%;
        }
    }
    @media ( max-width: 670px ) {
        #floating-bar{
            width: 70%;
        }
    }
    @media ( max-width: 600px ) {
        #floating-bar{
            right: 5%;
            width: 80%;
        }
        #floating-bar > button{
            font-size: 10pt;
        }
    }

</style>
<body class="no-sidebar is-preload">
<div id="page-wrapper">

    <!-- Header -->
    <section id="essaytitle">
        <div class="container">
            <!-- Logo -->
            <h3 style="word-break: keep-all">{{title}}</h3>
            <p style="text-align: center; margin-bottom: 2px">
                {{#if category0}} {{category0}} {{/if}}
                {{#if category1}} / {{category1}} {{/if}}
